import java.util.Scanner; //library imported for input purpose
class integer {
    public static void main(String[] args) {
    Scanner inp = new Scanner(System.in); //taking input
    int a = inp.nextInt();
    int dubl = a; //dublcation of variable a
    int Largest_digit = 0;
    while( dubl != 0){
        int Last_digit = dubl % 10;//extracting last_digit
        if( Last_digit > Largest_digit){
            Largest_digit = Last_digit;
        }
        dubl = dubl / 10;
    }
    System.out.println(" Largest digit: " + Largest_digit);
}
};

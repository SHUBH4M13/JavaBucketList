import java.util.Scanner; //library imported for input purpose
class integer {
    public static void main(String[] args) {
    Scanner inp = new Scanner(System.in); //taking input
    int a = inp.nextInt();
    int dubl = a; //dublcation of variable a
    int count = 0;
    while( dubl != 0){
        dubl = dubl / 10;
        count++;
    }
    System.out.println("Count : " + count);
}
};
